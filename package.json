{
  "name": "node-typescript-starter",
  "version": "0.4.0",
  "author": "Freek Mencke",
  "homepage": "https://twitter.com/FreekMencke",
  "repository": {
    "type": "git",
    "url": "https://github.com/FreekMencke/node-typescript-starter"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack --progress --env.development --env.nodemon",
    "start:prod": "webpack --progress --env.nodemon",
    "build:prod": "webpack --progress",
    "build:ci": "webpack",
    "build:docker": "docker build --rm -t toxsickcoder/node-typescript-starter:latest .",
    "deploy:docker": "sh ./scripts/deploy.sh",
    "analyse:dev": "webpack --progress --env.development --env.analyse && start dist/report.html",
    "analyse:prod": "webpack --progress --env.analyse && start dist/report.html",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jssha": "^3.1.0",
    "koa": "^2.5.1",
    "koa-body": "^1.7.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.3.2",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.4.0",
    "koa-session2": "^2.2.6",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.10",
    "log4js": "^2.5.3",
    "mongoose": "^5.0.17",
    "sha1": "^1.1.1",
    "wechat": "^2.1.0",
    "wechat-oauth": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.35",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "clean-webpack-plugin": "^3.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "nodemon-webpack-plugin": "^4.3.1",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
